{
    "name": "fooplugins/foogallery-migrate",
    "description": "FooGallery Migrate",
    "type": "library",
    "autoload"    : {
        "psr-4" : {
            "FooPlugins\\FooGalleryMigrate\\" : "includes"
        },
	  	"classmap": [ "includes" ]
    },
    "extra": {
        "installer-paths": {
            "vendor/{$name}/": ["type:wordpress-plugin"]
        }
    },
    "license": "GPL 3.0",
    "authors": [
        {
            "name": "Brad Vincent"
        }
    ],
    "config": {
